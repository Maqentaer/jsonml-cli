{
  "name": "jsonml-cli",
  "version": "0.1.0",
  "description": "Convert HTML to JSONML or vice versa",
  "main": "./jsonml",
  "bin": {
    "jsonml": "./bin/jsonml"
  },
  "dependencies": {
    "html2jsonml": "0.0.4",
    "request": "~2.33.0",
    "commander": "~2.1.0"
  },
  "scripts": {
    "start": "node ./index.js",
    "test": "node ./test/index.js",
    "watch": "nodemon -w ./index.js -w ./lib/json2html.js index.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maqentaer/jsonml-cli.git"
  },
  "keywords": [
    "jsonml",
    "html2jsonml",
    "jsonml2html"
  ],
